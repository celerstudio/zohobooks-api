{
    "name": "opsway/zohobooks-api",
    "description": "PHP SDK for ZohoBooks API",
    "license": "MIT",
    "keywords": [
        "Zoho Books",
        "API",
        "SDK",
        "library"
    ],
    "prefer-stable": true,
    "require": {
        "php": "^5.6 | ^7.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "friendsofphp/php-cs-fixer": "^2.0@dev"
    },
    "autoload": {
        "psr-4": {
            "OpsWay\\ZohoBooks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "OpsWay\\ZohoBooks\\Tests\\": "test/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
